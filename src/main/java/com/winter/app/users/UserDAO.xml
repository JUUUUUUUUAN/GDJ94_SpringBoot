<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	<mapper namespace="com.winter.app.users.UserDAO">
		<insert id="register" parameterType="UserDTO">
			INSERT INTO users VALUES (#{username}, #{password}, #{name}, #{email}, #{phone}, #{birth});
		</insert>
		<!-- fileAdd -->
		<insert id="fileAdd" parameterType="UserFileDTO">
			INSERT INTO profile VALUES (null, #{fileName}, #{FileOrigin}, #{username})
		</insert>
	</mapper>